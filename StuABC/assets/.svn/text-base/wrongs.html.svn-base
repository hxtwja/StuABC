<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>无标题文档</title>
<style type="text/css">
html,body,div,span,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,address,big,cite,code,del,em,font,img,ins,small,strong,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend
	{
	padding: 0;
	margin: 0;
}

.clearfix:after,.complex-problem .items:after {
	clear: both;
	content: ".";
	display: block;
	height: 0;
	visibility: hidden;
}

.complex-problem {
	width: 782px;
}

.complex-problem .items {
	margin-bottom: 20px;
}

.complex-problem .radio {
	border: 0;
	height: 44px;
	width: 60px;
	float: left;
	background: url(wimg/radio.png) left top no-repeat;
}

.complex-problem .radio-current {
	background: url(wimg/radio_current.png) left top no-repeat;
}

.complex-problem .cont {
	height: 44px;
	line-height: 44px;
	width: 722px;
	float: left;
	background: url(wimg/bg_items.png) left top no-repeat;
}

.complex-problem .cont-items {
	font-size: 16px;
}

.complex-problem .cont-items .td1,.complex-problem .cont-items .td2,.complex-problem .cont-items .td3,.complex-problem .cont-items .td4,.td3,.complex-problem .cont-items .td5
	{
	float: left;
	width: 130px;
	font-weight: bold;
	overflow: hidden;
	text-align: center;
	margin-right:15px;
}

.complex-problem .cont-items .td5 {
	padding-left: 45px;
	width: 70px;
}

.complex-problem .cont-items .td-in {
	padding: 0 10px;
	display: block;
	text-overflow: ellipsis;
	-o-text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
}

.complex-problem .sub-cont {
	clear: both;
	background-color: white;
	margin-right: 10px;
	border: 1px solid #fcdc92;
	padding: 5px 10px;
	margin: -1px 23px 0px 58px;
	margin-top: -1px;
	line-height: 2.2em;
	border-top: 0;
	display: none;
}

.btn-todo {
	border: 0;
	width: 60px;
	height: 32px;
	background: url(wimg/todo.png);
	display: block;
}

a.btn-todo:active {
	background: url(wimg/todo_current.png) left top no-repeat;
}

.complex-problem .sub-cont h3,.complex-problem .sub-cont p {
	font-size: 16px;
	font-weight: normal;
}

.complex-problem .sub-cont p {
	font-size: 16px;
}

.complex-problem .questions-lists li {
	padding-right: 10px;
	display: inline;
}
</style>
<script src="file:///android_asset/js/jquery-1.8.1.min.js"
	type="text/javascript"></script>
<!--<script src="http://code.jquery.com/jquery-1.7.2.min.js"></script>-->
<script type="text/javascript">
	$(document).ready(function() {
		$(".complex-problem .items .radio").toggle(function() {
			$(this).addClass("radio-current");
		}, function() {
			$(this).removeClass("radio-current");
		});
		$(".complex-problem .items .cont").toggle(function() {
			$(this).next(".sub-cont").show();
		}, function() {
			$(this).next(".sub-cont").hide();
		});
	});
	function doTest(str) {
		window.wrongs_java.doTest(str);
		return false;
	}
	function doSelect(str) {
		window.wrongs_java.doSelect(str);
	}
	function doSelectAll(str) {
		if (str == "1") {
			$(".complex-problem .items .radio").addClass("radio-current");
		} else if (str == "0") {
			$(".complex-problem .items .radio").removeClass("radio-current");
		}

	}
</script>

</head>

<body>
	<div class="complex-problem">{@timuDiv@}</div>
</body>
</html>